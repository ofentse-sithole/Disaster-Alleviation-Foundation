            <td>
                @Html.DisplayFor(modelItem => item.Disaster_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GoodsDonation_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Goods_Items)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Allocate_Date)
            </td>
