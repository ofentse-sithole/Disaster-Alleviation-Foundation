@model IEnumerable<Disaster_Allievation_Foundation_.Models.Allocation_Goods>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Disaster_ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GoodsDonation_ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Goods_Items)
